{
    "name": "scaleupstack/annotations",
    "description": "Annotations parser for classical DocBlock-style annotations and an extended multi-line declaration.",
    "type": "library",
    "keywords": [
        "annotations",
        "parser",
        "docblock",
        "phpdoc",
        "metadata",
        "reflection"
    ],
    "autoload": {
        "psr-4": {
            "ScaleUpStack\\Annotations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScaleUpStack\\Annotations\\Tests\\": "tests/"
        }
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Thomas Nunninger",
            "email": "thomas.nunninger@scaleupventures.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "scaleupstack/assert": "^1."
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
